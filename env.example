# 豆包API配置 (必需)
DOUBAO_API_KEY=your_doubao_api_key_here

# 数据库配置 (必需)
MEMFIREDB_URL=your_memfiredb_url_here
MEMFIREDB_ANON_KEY=your_memfiredb_anon_key_here

# 阿里云OSS配置 (必需)
OSS_REGION=oss-cn-hangzhou
OSS_ACCESS_KEY_ID=your_oss_access_key_id_here
